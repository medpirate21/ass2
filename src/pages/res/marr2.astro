<style>
  .content-wrapper {
    position: relative;
    z-index: 1;
    max-width: 800px;
    margin: 0 auto;
    padding: 2rem 1rem;
  }

  .main-title {
    font-size: 2.5rem;
    text-align: center;
    margin-bottom: 2rem;
    background: var(--glass-gradient-text);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    font-weight: 700;
    text-shadow: 0 0 30px rgba(139, 92, 246, 0.3);
  }

  .sections-list {
    position: relative;
    z-index: 1;
    display: flex;
    flex-direction: column;
    gap: 1rem;
    flex: 1;
    overflow: hidden;
  }

  .section-wrapper {
    position: relative;
    z-index: 2;
    border-radius: var(--glass-border-radius);
    background: var(--glass-bg);
    backdrop-filter: var(--glass-backdrop-filter);
    -webkit-backdrop-filter: var(--glass-backdrop-filter);
    border: var(--glass-border);
    box-shadow: var(--glass-shadow);
    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    display: flex;
    flex-direction: column;
    overflow: hidden;
  }

  .section-wrapper:hover {
    transform: translateY(-2px);
    box-shadow: var(--glass-shadow-hover);
  }

  .section-wrapper.active {
    margin: 1rem 0;
    background: var(--glass-bg-active);
    box-shadow: var(--glass-shadow-active);
  }

  .section-button {
    position: relative;
    width: 100%;
    display: flex;
    align-items: center;
    gap: 1rem;
    padding: 1.25rem;
    background: transparent;
    color: var(--glass-text);
    cursor: pointer;
    border: none;
    border-radius: var(--glass-border-radius);
    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    overflow: hidden;
  }

  .section-button::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: var(--glass-shimmer);
    transition: left 0.5s ease;
    z-index: 0;
  }

  .section-button:hover::before {
    left: 100%;
  }

  .section-button > * {
    position: relative;
    z-index: 3;
  }

  .section-wrapper.active .section-button {
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
    border-bottom: 1px solid var(--glass-border-color);
  }

  .section-button .number {
    font-family: 'JetBrains Mono', monospace;
    color: var(--glass-accent);
    font-weight: 600;
    font-size: 1.1rem;
    text-shadow: 0 0 10px var(--glass-accent-glow);
  }

  .section-button .text {
    font-size: 1.1rem;
    font-weight: 600;
    color: var(--glass-text);
    position: relative;
    z-index: 5;
  }

  .chevron {
    margin-left: auto;
    transition: transform 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    color: var(--glass-accent);
    filter: drop-shadow(0 0 5px var(--glass-accent-glow));
  }

  .section-wrapper.active .chevron {
    transform: rotate(180deg);
  }

  .section-content {
    position: relative;
    z-index: 1;
    height: 0;
    opacity: 0;
    visibility: hidden;
    transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
    background: var(--glass-bg-secondary);
    backdrop-filter: var(--glass-backdrop-filter);
    -webkit-backdrop-filter: var(--glass-backdrop-filter);
    border-bottom-left-radius: var(--glass-border-radius);
    border-bottom-right-radius: var(--glass-border-radius);
    overflow: hidden;
  }

  .section-wrapper.active .section-content {
    height: auto;
    opacity: 1;
    visibility: visible;
    padding-bottom: 1rem;
  }

  .button-list {
    display: flex;
    flex-direction: column;
    gap: 0.75rem;
    padding: 1.25rem;
    max-height: calc(70vh - 2rem);
    overflow-y: auto;
    overflow-x: hidden;
    margin-bottom: 0.5rem;
  }

  .button-list::-webkit-scrollbar {
    width: 8px;
  }

  .button-list::-webkit-scrollbar-track {
    background: var(--glass-bg-secondary);
    border-radius: 4px;
    margin: 4px;
  }

  .button-list::-webkit-scrollbar-thumb {
    background: var(--glass-accent);
    border-radius: 4px;
    border: 2px solid transparent;
    background-clip: padding-box;
  }

  .button-list::-webkit-scrollbar-thumb:hover {
    background: var(--glass-accent-hover);
    background-clip: padding-box;
  }

  .btn {
    text-align: left;
    padding: 1.5rem 1.75rem;
    font-size: 1.1rem;
    background: var(--glass-bg-tertiary);
    backdrop-filter: var(--glass-backdrop-filter-light);
    -webkit-backdrop-filter: var(--glass-backdrop-filter-light);
    border: var(--glass-border-light);
    border-radius: var(--glass-border-radius-small);
    color: var(--glass-text);
    cursor: pointer;
    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
    opacity: 0;
    transform: translateY(-10px);
    position: relative;
    overflow: hidden;
    font-weight: 500;
    min-height: 60px;
  }

  .btn::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: var(--glass-shimmer-subtle);
    transition: left 0.4s ease;
    z-index: 0;
  }

  .btn:hover::before {
    left: 100%;
  }

  .btn > * {
    position: relative;
    z-index: 1;
  }

  .section-wrapper.active .btn {
    opacity: 1;
    transform: translateY(0);
  }

  .btn:hover {
    background: var(--glass-bg-hover);
    border-color: var(--glass-accent);
    transform: translateY(-2px);
    box-shadow: var(--glass-shadow-button);
  }

  .btn.highlight {
    background: var(--glass-bg-active);
    border-color: var(--glass-accent);
    transform: scale(1.02);
    box-shadow: var(--glass-shadow-button-active);
  }

  .section-heading {
    font-size: 1.2rem;
    font-weight: 700;
    color: var(--glass-accent);
    text-align: center;
    margin: 1rem 0 0.5rem 0;
    padding: 0.5rem;
    background: var(--glass-bg-accent);
    border-radius: var(--glass-border-radius-small);
    border: var(--glass-border-light);
    text-shadow: 0 0 10px var(--glass-accent-glow);
  }

  @media (max-width: 768px) {
    .content-wrapper {
      padding: 1rem 0.5rem;
    }
    
    .main-title {
      font-size: 2rem;
    }
    
    .section-button {
      padding: 1rem;
    }
    
    .btn {
      padding: 0.875rem 1rem;
      font-size: 0.9rem;
    }
    
    .button-list {
      padding: 1rem;
      max-height: calc(60vh - 2rem);
    }
  }

</style>

