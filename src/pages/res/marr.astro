<script src="/src/config.js"></script>
<script>
  document.addEventListener('DOMContentLoaded', function() {
    console.log('DOM loaded - marr.astro');
    setupDropdowns();
  });

  document.addEventListener('astro:page-load', function() {
    console.log('Astro page loaded - marr.astro');
    setTimeout(setupDropdowns, 100);
  });

  function setupDropdowns() {
    console.log('Setting up dropdowns - marr.astro');
    const sectionButtons = document.querySelectorAll('.section-button');
    console.log('Found buttons:', sectionButtons.length);
    
    sectionButtons.forEach((button, index) => {
      // Remove existing listeners by cloning
      const newButton = button.cloneNode(true);
      button.parentNode?.replaceChild(newButton, button);
      
      newButton.addEventListener('click', function(e) {
        console.log('Dropdown clicked - marr.astro');
        e.preventDefault();
        
        const wrapper = this.closest('.section-wrapper');
        const wasActive = wrapper?.classList.contains('active');

        document.querySelectorAll('.section-wrapper').forEach(w => {
          w.classList.remove('active');
        });

        if (!wasActive && wrapper) {
          wrapper.classList.add('active');
          wrapper.querySelectorAll('.btn').forEach((btn, index) => {
            (btn as HTMLElement).style.transitionDelay = `${index * 0.02}s`;
          });
        }
      });
    });
  }

  // Immediate setup if DOM ready
  if (document.readyState === 'complete') {
    setupDropdowns();
  }

  // Function to check video quota
  async function checkVideoQuota(userid: string): Promise<number> {
      try {
        const response = await fetch((window as any).APP_CONFIG.QUOTA_CHECK(userid), {
          method: 'GET',
          headers: {
            'Content-Type': 'application/json',
          }
        });

        if (!response.ok) {
          throw new Error(`Server responded with ${response.status}: ${response.statusText}`);
        }

        const quotaData = await response.json();
        return quotaData.videos_remaining || 0;
      } catch (error) {
        console.error('Error checking video quota:', error);
        return 0; // Default to 0 if error
      }
    }

    // Function to check note quota
    async function checkNoteQuota(userid: string): Promise<number> {
      try {
        const response = await fetch((window as any).APP_CONFIG.QUOTA_CHECK(userid), {
          method: 'GET',
          headers: {
            'Content-Type': 'application/json',
          }
        });

        if (!response.ok) {
          throw new Error(`Server responded with ${response.status}: ${response.statusText}`);
        }

        const quotaData = await response.json();
        return quotaData.notes_remaining || 0;
      } catch (error) {
        console.error('Error checking note quota:', error);
        return 0; // Default to 0 if error
      }
    }

    // Single delegated event listener for all video buttons (data-class="video")
    document.addEventListener('click', async (e) => {
      if (e.target && (e.target as Element).closest) {
        const button = (e.target as Element).closest('button[data-class="video"]') as HTMLButtonElement;
        if (button) {
          e.preventDefault();
          const dataId = button.getAttribute('data-id');
          const title = button.textContent?.trim();
          
          console.log('Video button clicked:', dataId, title);
          
          if (dataId && title) {
            // Get user data from localStorage
            const storedUser = localStorage.getItem('medoracle_user');
            if (!storedUser) {
              // User not logged in, show login modal
              if (window.loginModal) {
                window.loginModal.show();
              }
              return;
            }

            try {
              const userData = JSON.parse(storedUser);
              const userid = userData.id;

              // Only check video quota - don't decrement yet
              const videosRemaining = await checkVideoQuota(userid);
              
              if (videosRemaining <= 0) {
                // No quota remaining, redirect to limit reached page
                window.location.href = '/limit-reached';
                return;
              }

              // Navigate to video page without decrementing quota
              const encodedId = btoa(dataId);
              const encodedTitle = encodeURIComponent(title);
              const encodedUserId = btoa(userid);
              window.location.href = `/video?id=${encodedId}&title=${encodedTitle}&userid=${encodedUserId}`;
              
            } catch (error) {
              console.error('Error processing video request:', error);
              // On error, still allow access but log the issue
              const encodedId = btoa(dataId);
              const encodedTitle = encodeURIComponent(title);
              window.location.href = `/video?id=${encodedId}&title=${encodedTitle}`;
            }
          }
        }
      }
    });

    // Single delegated event listener for all PDF buttons (data-class="pdf")
    document.addEventListener('click', async (e) => {
      if (e.target && (e.target as Element).closest) {
        const button = (e.target as Element).closest('button[data-class="pdf"]') as HTMLButtonElement;
        if (button) {
          e.preventDefault();
          const dataId = button.getAttribute('data-id');
          const title = button.textContent?.trim();
          
          console.log('PDF button clicked:', dataId, title);
          
          if (dataId && title) {
            // Get user data from localStorage
            const storedUser = localStorage.getItem('medoracle_user');
            if (!storedUser) {
              // User not logged in, show login modal
              if (window.loginModal) {
                window.loginModal.show();
              }
              return;
            }

            try {
              const userData = JSON.parse(storedUser);
              const userid = userData.id;

              // Only check note quota - don't decrement yet
              const notesRemaining = await checkNoteQuota(userid);
              
              if (notesRemaining <= 0) {
                // No quota remaining, redirect to limit reached page
                window.location.href = '/limit-reached';
                return;
              }

              // Navigate to note page without decrementing quota
              const encodedId = btoa(dataId);
              const encodedTitle = encodeURIComponent(title);
              const encodedUserId = btoa(userid);
              window.location.href = `/notepage?id=${encodedId}&title=${encodedTitle}&userid=${encodedUserId}`;
              
            } catch (error) {
              console.error('Error processing note request:', error);
              // On error, still allow access but log the issue
              const encodedId = btoa(dataId);
              const encodedTitle = encodeURIComponent(title);
              window.location.href = `/notepage?id=${encodedId}&title=${encodedTitle}`;
            }
          }
        }
      }
    });
</script>
